<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity OsGroup.
    -->
    <changeSet id="20200701202639-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="attributes_id"
                                 baseTableName="os_group"
                                 constraintName="fk_os_group_attributes_id"
                                 referencedColumnNames="id"
                                 referencedTableName="attribute"/>

        <addForeignKeyConstraint baseColumnNames="os_subgroups_id"
                                 baseTableName="os_group"
                                 constraintName="fk_os_group_os_subgroups_id"
                                 referencedColumnNames="id"
                                 referencedTableName="os_subgroup"/>

        <addForeignKeyConstraint baseColumnNames="applets_id"
                                 baseTableName="os_group"
                                 constraintName="fk_os_group_applets_id"
                                 referencedColumnNames="id"
                                 referencedTableName="applet"/>

    </changeSet>
</databaseChangeLog>
